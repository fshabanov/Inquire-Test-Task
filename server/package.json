{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:js": "npx eslint \"src/**/*.ts\"",
    "lint:type": "npx tsc --noEmit",
    "lint": "npm run lint:js && npm run lint:type",
    "typeorm": "typeorm-ts-node-esm -d ./src/typeorm.config.ts",
    "typeorm:migrate": "npm run typeorm -- migration:run",
    "typeorm:revert": "npm run typeorm -- migration:revert",
    "typeorm:generate": "npm run typeorm -- migration:generate ./src/data/migrations/$npm_config_name"
  },
  "dependencies": {
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.4.0",
    "@nestjs/platform-express": "9.4.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "typeorm": "0.3.14"
  },
  "devDependencies": {
    "@nestjs/cli": "9.4.0",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "prettier": "2.8.7",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
